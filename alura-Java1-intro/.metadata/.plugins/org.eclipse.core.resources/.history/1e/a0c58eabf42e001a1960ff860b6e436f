
public class TestaVariaveis {
	//statement não funciona fora dos métodos
	public static void main(String[] args) {
		System.out.println("Novo teste");
		
		int idade;
		idade = 20 + 10;
		
		System.out.println(idade);
		
		idade = (7 * 5) + 2;
		
		System.out.println(idade);
		
		System.out.println("A idade é" + idade);
		
	}
}
